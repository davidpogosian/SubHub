{
    "apphosting": {
        "backendId": "subhub",
        "rootDir": "/",
        "ignore": [
            "node_modules",
            ".git",
            "firebase-debug.log",
            "firebase-debug.*.log",
            "functions"
        ],
        "alwaysDeployFromSource": true
    },
    "firestore": {
        "database": "subhub",
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    }
}